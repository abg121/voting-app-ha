          "refId": "A"
        }
      ]
    },
    {
      "title": "Redis Health",
      "type": "stat",
      "gridPos": {
        "h": 6,
        "w": 4,
        "x": 4,
        "y": 0
      },
      "targets": [
        {
          "expr": "redis_up",
          "legendFormat": "Redis",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "steps": [
              {
                "color": "red",
